<style>
.summit-logo--animated {
  max-width: 10em;
  animation: 2s forwards zoom,22s linear 2s infinite anticlockwise
}

.summit-logo__mid {
  animation: 10s linear 2s infinite clockwise
}

.summit-logo__inner {
  animation: 12s linear 2s infinite anticlockwise
}

@keyframes zoom {
  0% {
    transform: scale(0)
  }

  to {
    transform: scale(1)
  }
}

@keyframes clockwise {
  0% {
    transform-origin: 50%;
    transform: rotate(0)
  }

  to {
    transform-origin: 50%;
    transform: rotate(360deg)
  }
}

@keyframes anticlockwise {
  0% {
    transform-origin: 50%;
    transform: rotate(0)
  }

  to {
    transform-origin: 50%;
    transform: rotate(-360deg)
  }
}
</style>
