{
  "kind": "singleType",
  "collectionName": "media_pages",
  "info": {
    "singularName": "media-page",
    "pluralName": "media-pages",
    "displayName": "Media Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "media_hero": {
      "type": "component",
      "component": "shared.hero-section",
      "repeatable": false
    },
    "media_content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultMarkdown"
      }
    },
    "media_cta": {
      "type": "component",
      "component": "shared.cta-link",
      "repeatable": false
    },
    "dynamic": {
      "type": "dynamiczone",
      "components": ["shared.section", "shared.cta-link"]
    }
  }
}
